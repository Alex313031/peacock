{
	"name": "peacock",
	"appName": "Peacock",
	"productName": "Peacock",
	"version": "2.6.0",
	"author": "Alex313031 <alex313031@gmail.com>",
	"license": "MIT",
	"description": "Open source, experimental, private-by-default web browser built with Electron.",
	"main": "main.js",
	"keywords": [
		"browser",
		"electron",
		"web",
		"security",
		"privacy",
		"internet",
		"anonymity"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/Alex313031/peacock"
	},
	"scripts": {
		"start": "electron --no-sandbox .",
		"clean": "rimraf dist && rimraf node_modules",
		"distclean": "rimraf dist",
		"burn": "rimraf package-lock.json",
		"build": "electron-builder --dir",
		"distlinux": "electron-builder --l",
		"distwin": "electron-builder -w",
		"distmac": "electron-builder -m"
	},
	"devDependencies": {
		"asar": "3.2.0",
		"electron": "12.2.3",
		"electron-builder": "22.14.13",
		"rimraf": "3.0.2"
	},
	"dependencies": {
		"@cliqz/adblocker": "1.20.6",
		"@cliqz/adblocker-content": "1.20.6",
		"@cliqz/adblocker-electron": "1.20.6",
		"@cliqz/adblocker-electron-preload": "1.20.6",
		"@cliqz/adblocker-extended-selectors": "1.20.6",
		"@electron/remote": "2.1.2",
		"cross-fetch": "3.1.8",
		"electron-context-menu": "2.5.2",
		"electron-store": "5.2.0",
		"sortablejs": "1.15.2",
		"uuid": "8.3.2",
		"v8-compile-cache": "2.4.0"
	},
	"resolutions": {
		"electron": "12.2.3"
	}
